
import java.util.*;
import java.io.*;


public class Graph
{
		private int V;
		private ArrayList<Integer> adj[];

		public Graph(int k)
		{
			V=k;
			adj=new LinkedList[V];	

			for(int i=0;i<V;i++)	
			{
				adj[i]=new ArrayList();
			}	

		}


		public void addEdge(int v,int w)
		{
			adj[v].add(w);
			adj[w].add(v);
		}

		public static void main(String args[])
		{

				Graph g=new Graph();

				g.addEdge();

		}


}
